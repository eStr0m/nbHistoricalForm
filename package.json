{
  "devDependencies": {
    "concurrently": "^8.2.1"
  },
  "scripts": {
    "start:backend": "cd server && node app.js",
    "start:frontend": "cd historicalform && npm start",
    "dev":  "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "install:backend": "cd server && npm install",
    "install:frontend": "cd historicalform && npm install",
    "dev-install": "concurrently \"npm run install:backend\" \"npm run install:frontend\""
  }
}
